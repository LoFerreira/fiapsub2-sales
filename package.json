{
  "name": "fiapsub2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "tsc-watch --onSuccess \"node --env-file=.env ./lib/index.js\"",
    "start:prod": "node ./lib/index.js",
    "build": "tsc",
    "build:clean": "rm -rf lib && npm run build",
    "docker:build": "docker build -t fiapsub2 .",
    "docker:run": "docker run -p 3000:3000 --env-file .env fiapsub2",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:prod": "docker-compose up --build -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "mongodb": "^6.19.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/typescript": "^0.4.29",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "tsc-watch": "^7.1.1",
    "typescript": "^5.9.2"
  }
}
